  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Pistou Back End (Cargo launch)",
      "cargo": {
        "args": ["build", "--manifest-path", "backend/Cargo.toml"]
      },
      "program": "${cargo:program}",
      "args": [],
      "env": {
        "TOKEN": "token",
        "RUST_LOG": "debug"
      },
      "cwd": "${workspaceRoot}/backend"
    },
    {
      "name": "Pistou Front End (Flutter)",
      "program": "lib/main.dart",
      "request": "launch",
      "type": "dart",
      "args": ["--web-port", "8001"],
      "cwd": "${workspaceRoot}/frontend"
    }
  ]
}
